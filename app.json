{
  "expo": {
    "name": "Love Is Free",
    "slug": "love-is-free",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.loveisfree",
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to your camera for video calls.",
        "NSMicrophoneUsageDescription": "We need access to your microphone for audio and video calls."
      }
    },
    "android": {
      "package": "com.yourcompany.loveisfree",
      "permissions": ["CAMERA", "RECORD_AUDIO", "MODIFY_AUDIO_SETTINGS"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "93cad262-1a95-4bf6-b55c-2facc0317c06"
      }
    }
  }
}
